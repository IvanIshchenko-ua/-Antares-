xDEV(\"strong\", {\n            children: \" \\u0423\\u043C\\u0430\\u043D\\u0441\\u044C\\u043A\\u0456\\u0439 \\u0434\\u0438\\u0442\\u044F\\u0447\\u0456\\u0439 \\u0448\\u043A\\u043E\\u043B\\u0456 \\u043C\\u0438\\u0441\\u0442\\u0435\\u0446\\u0442\\u0432 \\xAB\\u0410\\u041D\\u0422\\u0410\\u0420\\u0415\\u0421\\xBB.\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 116,\n            columnNumber: 13\n          }, this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumber: 113,\n          columnNumber: 11\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 103,\n        columnNumber: 9\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 59,\n      columnNumber: 7\n    }, this)]\n  }, void 0, true, {\n    fileName: _jsxFileName,\n    lineNumber: 5,\n    columnNumber: 5\n  }, this);\n};\n_c = About;\nexport default About;\nvar _c;\n$RefreshReg$(_c, \"About\");","map":{"version":3,"names":["React","jsxDEV","_jsxDEV","About","className","style","padding","maxWidth","margin","fontFamily","lineHeight","color","children","textAlign","marginBottom","fontSize","fileName","_jsxFileName","lineNumber","columnNumber","display","flexWrap","gap","alignItems","justifyContent","src","alt","width","borderRadius","boxShadow","flex","minWidth","marginTop","background","borderLeft","fontStyle","_c","$RefreshReg$"],"sources":["/Users/ivanisenko/Desktop/Narnia/робота/Не закінчені проекти/Мама проект школа/-Antares-/Frontend/src/components/static/About.js"],"sourcesContent":["import React from 'react';\n\nconst About = () => {\n  return (\n    <div\n      className=\"about-page\"\n      style={{\n        padding: '3rem 1rem',\n        maxWidth: '1100px',\n        margin: '0 auto',\n        fontFamily: \"'Segoe UI', sans-serif\",\n        lineHeight: 1.7,\n        color: '#2c3e50'\n      }}\n    >\n      <h1\n        style={{\n          textAlign: 'center',\n          marginBottom: '2rem',\n          color: '#1a202c',\n          fontSize: '2.2rem'\n        }}\n      >\n        Про школу мистецтв «Антарес»\n      </h1>\n\n      <div\n        style={{\n          display: 'flex',\n          flexWrap: 'wrap',\n          gap: '2rem',\n          alignItems: 'center',\n          justifyContent: 'center',\n          marginBottom: '2.5rem'\n        }}\n      >\n        <img\n          src=\"/img/school-building.jpg\"\n          alt=\"Будівля школи\"\n          style={{\n            width: '400px',\n            borderRadius: '14px',\n            boxShadow: '0 6px 20px rgba(0,0,0,0.1)'\n          }}\n        />\n        <div style={{ flex: 1, minWidth: '300px' }}>\n          <p style={{ fontSize: '1.1rem', marginBottom: '1rem' }}>\n            <strong>Ми одна із найкращих шкіл області!</strong>\n          </p>\n          <p style={{ fontSize: '1.05rem' }}>\n            Унікальним надбанням Умані мистецької є навчальний заклад\n            <strong> Уманська дитяча школа мистецтв «Антарес»</strong> — один із\n            найбільших початкових спеціалізованих мистецьких навчальних закладів\n            Черкащини з контингентом понад <strong>750 учнів</strong>.\n          </p>\n        </div>\n      </div>\n\n      <div style={{ fontSize: '1.05rem' }}>\n        <p>\n          Ми досягаємо успіхів, об'єднавши надзвичайно творчих,\n          висококваліфікованих викладачів та талановитих учнів.\n        </p>\n\n        <p>\n          У школі працюють музичні (сольно–хорове мистецтво та гра на різних\n          музичних інструментах), художній та хореографічний відділи.\n        </p>\n\n        <p>\n          На музичних відділах створено <strong>35 вокальних та інструментальних\n          творчих колективів</strong> (оркестр народних інструментів, ансамбль\n          скрипалів, баяністів, гітаристів, віолончелістів, духових та ударних\n          інструментів, хори старших та молодших класів, вокальні ансамблі:\n          «Перлина», «Оксамит», «Жарптиця», «Глорія», «Камелія», «Дивограй»\n          (старша група), «Дивограй» (молодша група), «Смайл», «Веселкограй»,\n          «Мальви», «Краплинки», «Промінь», «Соколики» академічного та естрадного\n          співу та інші малі форми).\n        </p>\n\n        <p>\n          Дитячі колективи школи є окрасою міських та обласних заходів, свят,\n          концертів та урочистостей.\n        </p>\n\n        <p>\n          Щорічні виставки робіт учнів художнього відділу у Художньому музеї\n          міста налічують понад <strong>150 картин</strong>.\n        </p>\n\n        <p>\n          Уманська дитяча школа мистецтв пишається своїми вихованцями, адже\n          кожного року близько <strong>150 учнів</strong> стають лауреатами та\n          переможцями міжнародних, всеукраїнських фестивалів і конкурсів.\n          Найкращі учні школи мають змогу <strong>безкоштовно навчатися</strong>.\n        </p>\n\n        <p>\n          Після закінчення навчання в школі, випускники отримують\n          <strong> свідоцтво державного зразка</strong>.\n        </p>\n\n        <div\n          style={{\n            marginTop: '2rem',\n            padding: '1.5rem',\n            background: '#f9fafb',\n            borderLeft: '6px solid #2563eb',\n            borderRadius: '8px',\n            fontStyle: 'italic'\n          }}\n        >\n          <p>\n            Ми бажаємо майбутнім учням та їх батькам відкрити для себе\n            неповторний, яскравий та захоплюючий світ мистецтва в\n            <strong> Уманській дитячій школі мистецтв «АНТАРЕС».</strong>\n          </p>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default About;\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE1B,MAAMC,KAAK,GAAGA,CAAA,KAAM;EAClB,oBACED,OAAA;IACEE,SAAS,EAAC,YAAY;IACtBC,KAAK,EAAE;MACLC,OAAO,EAAE,WAAW;MACpBC,QAAQ,EAAE,QAAQ;MAClBC,MAAM,EAAE,QAAQ;MAChBC,UAAU,EAAE,wBAAwB;MACpCC,UAAU,EAAE,GAAG;MACfC,KAAK,EAAE;IACT,CAAE;IAAAC,