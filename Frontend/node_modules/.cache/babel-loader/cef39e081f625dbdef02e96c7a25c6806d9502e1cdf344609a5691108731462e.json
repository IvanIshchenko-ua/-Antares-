\u043D\\u043E \\u043D\\u0430\\u0432\\u0447\\u0430\\u0442\\u0438\\u0441\\u044F\"}),\".\"]}),/*#__PURE__*/_jsxs(\"p\",{children:[\"\\u041F\\u0456\\u0441\\u043B\\u044F \\u0437\\u0430\\u043A\\u0456\\u043D\\u0447\\u0435\\u043D\\u043D\\u044F \\u043D\\u0430\\u0432\\u0447\\u0430\\u043D\\u043D\\u044F \\u0432 \\u0448\\u043A\\u043E\\u043B\\u0456, \\u0432\\u0438\\u043F\\u0443\\u0441\\u043A\\u043D\\u0438\\u043A\\u0438 \\u043E\\u0442\\u0440\\u0438\\u043C\\u0443\\u044E\\u0442\\u044C\",/*#__PURE__*/_jsx(\"strong\",{children:\" \\u0441\\u0432\\u0456\\u0434\\u043E\\u0446\\u0442\\u0432\\u043E \\u0434\\u0435\\u0440\\u0436\\u0430\\u0432\\u043D\\u043E\\u0433\\u043E \\u0437\\u0440\\u0430\\u0437\\u043A\\u0430\"}),\".\"]}),/*#__PURE__*/_jsx(\"div\",{style:{marginTop:'2rem',padding:'1.5rem',background:'#f9fafb',borderLeft:'6px solid #2563eb',borderRadius:'8px',fontStyle:'italic'},children:/*#__PURE__*/_jsxs(\"p\",{children:[\"\\u041C\\u0438 \\u0431\\u0430\\u0436\\u0430\\u0454\\u043C\\u043E \\u043C\\u0430\\u0439\\u0431\\u0443\\u0442\\u043D\\u0456\\u043C \\u0443\\u0447\\u043D\\u044F\\u043C \\u0442\\u0430 \\u0457\\u0445 \\u0431\\u0430\\u0442\\u044C\\u043A\\u0430\\u043C \\u0432\\u0456\\u0434\\u043A\\u0440\\u0438\\u0442\\u0438 \\u0434\\u043B\\u044F \\u0441\\u0435\\u0431\\u0435 \\u043D\\u0435\\u043F\\u043E\\u0432\\u0442\\u043E\\u0440\\u043D\\u0438\\u0439, \\u044F\\u0441\\u043A\\u0440\\u0430\\u0432\\u0438\\u0439 \\u0442\\u0430 \\u0437\\u0430\\u0445\\u043E\\u043F\\u043B\\u044E\\u044E\\u0447\\u0438\\u0439 \\u0441\\u0432\\u0456\\u0442 \\u043C\\u0438\\u0441\\u0442\\u0435\\u0446\\u0442\\u0432\\u0430 \\u0432\",/*#__PURE__*/_jsx(\"strong\",{children:\" \\u0423\\u043C\\u0430\\u043D\\u0441\\u044C\\u043A\\u0456\\u0439 \\u0434\\u0438\\u0442\\u044F\\u0447\\u0456\\u0439 \\u0448\\u043A\\u043E\\u043B\\u0456 \\u043C\\u0438\\u0441\\u0442\\u0435\\u0446\\u0442\\u0432 \\xAB\\u0410\\u041D\\u0422\\u0410\\u0420\\u0415\\u0421\\xBB.\"})]})})]})]});};export default About;","map":{"version":3,"names":["React","jsx","_jsx","jsxs","_jsxs","About","className","style","padding","maxWidth","margin","fontFamily","lineHeight","color","children","display","flexWrap","gap","alignItems","justifyContent","marginBottom","src","alt","width","borderRadius","boxShadow","flex","minWidth","fontSize","marginTop","background","borderLeft","fontStyle"],"sources":["/Users/ivanisenko/Desktop/Narnia/робота/Не закінчені проекти/Мама проект школа/-Antares-/Frontend/src/components/static/About.js"],"sourcesContent":["import React from 'react';\n\nconst About = () => {\n  return (\n    <div\n      className=\"about-page\"\n      style={{\n        padding: '3rem 1rem',\n        maxWidth: '1100px',\n        margin: '0 auto',\n        fontFamily: \"'Segoe UI', sans-serif\",\n        lineHeight: 1.7,\n        color: '#2c3e50'\n      }}\n    >\n\n      <div\n        style={{\n          display: 'flex',\n          flexWrap: 'wrap',\n          gap: '2rem',\n          alignItems: 'center',\n          justifyContent: 'center',\n          marginBottom: '2.5rem'\n        }}\n      >\n        <img\n          src=\"/img/school-building.jpg\"\n          alt=\"Будівля школи\"\n          style={{\n            width: '400px',\n            borderRadius: '14px',\n            boxShadow: '0 6px 20px rgba(0,0,0,0.1)'\n          }}\n        />\n        <div style={{ flex: 1, minWidth: '300px' }}>\n          <p style={{ fontSize: '1.1rem', marginBottom: '1rem' }}>\n            <strong>Ми одна із найкращих шкіл області!</strong>\n          </p>\n          <p style={{ fontSize: '1.05rem' }}>\n            Унікальним надбанням Умані мистецької є навчальний заклад\n            <strong> Уманська дитяча школа мистецтв «Антарес»</strong> — один із\n            найбільших початкових спеціалізованих мистецьких навчальних закладів\n            Черкащини з контингентом понад <strong>750 учнів</strong>.\n          </p>\n        </div>\n      </div>\n\n      <div style={{ fontSize: '1.05rem' }}>\n        <p>\n          Ми досягаємо успіхів, об'єднавши надзвичайно творчих,\n          висококваліфікованих викладачів та талановитих учнів.\n        </p>\n\n        <p>\n          У школі працюють музичні (сольно–хорове мистецтво та гра на різних\n          музичних інструментах), художній та хореографічний відділи.\n        </p>\n\n        <p>\n          На музичних відділах створено <strong>35 вокальних та інструментальних\n          творчих колективів</strong> (оркестр народних інструментів, ансамбль\n          скрипалів, баяністів, гітаристів, віолончелістів, духових та ударних\n          інструментів, хори старших та молодших класів, вокальні ансамблі:\n          «Перлина», «Оксамит», «Жарптиця», «Глорія», «Камелія», «Дивограй»\n          (старша група), «Дивограй» (молодша група), «Смайл», «Веселкограй»,\n          «Мальви», «Краплинки», «Промінь», «Соколики» академічного та естрадного\n          співу та інші малі форми).\n        </p>\n\n        <p>\n          Дитячі колективи школи є окрасою міських та обласних заходів, свят,\n          концертів та урочистостей.\n        </p>\n\n        <p>\n          Щорічні виставки робіт учнів художнього відділу у Художньому музеї\n          міста налічують понад <strong>150 картин</strong>.\n        </p>\n\n        <p>\n          Уманська дитяча школа мистецтв пишається своїми вихованцями, адже\n          кожного року близько <strong>150 учнів</strong> стають лауреатами та\n          переможцями міжнародних, всеукраїнських фестивалів і конкурсів.\n          Найкращі учні школи мають змогу <strong>безкоштовно навчатися</strong>.\n        </p>\n\n        <p>\n          Після закінчення навчання в школі, випускники отримують\n          <strong> свідоцтво державного зразка</strong>.\n        </p>\n\n        <div\n          style={{\n            marginTop: '2rem',\n            padding: '1.5rem',\n            background: '#f9fafb',\n            borderLeft: '6px solid #2563eb',\n            borderRadius: '8px',\n            fontStyle: 'italic'\n          }}\n        >\n          <p>\n            Ми бажаємо майбутнім учням та їх батькам відкрити для себе\n            неповторний, яскравий та захоплюючий світ мистецтва в\n            <strong> Уманській дитячій школі мистецтв «АНТАРЕС».</strong>\n          </p>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default About;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE1B,KAAM,CAAAC,KAAK,CAAGA,CAAA,GAAM,CAClB,mBACED,KAAA,QACEE,SAAS,CAAC,YAAY,CACtBC,KAAK,CAAE,CACLC,OAAO,CAAE,WAAW,CACpBC,QAAQ,CAAE,QAAQ,CAClBC,MAAM,CAAE,QAAQ,CAChBC,UAAU,CAAE,wBAAwB,CACpCC,UAAU,CAAE,GAAG,CACfC,KAAK,CAAE,SACT,CAAE,CAAAC,QAAA,eAGFV,KAAA,QACEG,KAAK,CAAE,CACLQ,OAAO,CAAE,MAAM,CACfC,QAAQ,CAAE,MAAM,CAChBC,GAAG,CAAE,MAAM,CACXC,UAAU,CAAE,QAAQ,CACpBC,cAAc,CAAE,QAAQ,CACxBC,YAAY,CAAE,QAChB,CAAE,CAAAN,QAAA,eAEFZ,IAAA,QACEmB,GAAG,CAAC,0BAA0B,CAC9BC,GAAG,CAAC,2EAAe,CACnBf,KAAK,CAAE,CACLgB,KAAK,CAAE,OAAO,CACdC,YAAY,CAAE,MAAM,CACpBC,SAAS,CAAE,4BACb,CAAE,CACH,CAAC,cACFrB,KAAA,QAAKG,KAAK,CAAE,CAAEmB,IAAI,CAAE,CAAC,CAAEC,QAAQ,CAAE,OAAQ,CAAE,CAAAb,QAAA,eACzCZ,IAAA,MAAGK,KAAK,CAAE,CAAEqB,QAAQ,CAAE,QAAQ,CAAER,YAAY,CAAE,MAAO,CAAE,CAAAN,QAAA,cACrDZ,IAAA,WAAAY,QAAA,CAAQ,gLAAkC,CAAQ,CAAC,CAClD,CAAC,cACJV,KAAA,MAAGG,KAAK,CAAE,CAAEqB,QAAQ,CAAE,SAAU,CAAE,CAAAd,QAAA,EAAC,0TAEjC,cAAAZ,IAAA,WAAAY,QAAA,CAAQ,2NAAyC,CAAQ,CAAC,ulBAE3B,cAAAZ,IAAA,WAAAY,QAAA,CAAQ,oCAAS,CAAQ,CAAC,IAC3D,EAAG,CAAC,EACD,CAAC,EACH,CAAC,cAENV,KAAA,QAAKG,KAAK,CAAE,CAAEqB,QAAQ,CAAE,SAAU,CAAE,CAAAd,QAAA,eAClCZ,IAAA,MAAAY,QAAA,CAAG,8jBAGH,CAAG,CAAC,cAEJZ,IAAA,MAAAY,QAAA,CAAG,upBAGH,CAAG,CAAC,cAEJV,KAAA,MAAAU,QAAA,EAAG,kKAC6B,cAAAZ,IAAA,WAAAY,QAAA,CAAQ,iRACpB,CAAQ,CAAC,kgEAO7B,EAAG,CAAC,cAEJZ,IAAA,MAAAY,QAAA,CAAG,2eAGH,CAAG,CAAC,cAEJV,KAAA,MAAAU,QAAA,EAAG,4dAEqB,cAAAZ,IAAA,WAAAY,QAAA,CAAQ,0CAAU,CAAQ,CAAC,IACnD,EAAG,CAAC,cAEJV,KAAA,MAAAU,QAAA,EAAG,gdAEoB,cAAAZ,IAAA,WAAAY,QAAA,CAAQ,oCAAS,CAAQ,CAAC,knBAEf,cAAAZ,IAAA,WAAAY,QAAA,CAAQ,2HAAqB,CAAQ,CAAC,IACxE,EAAG,CAAC,cAEJV,KAAA,MAAAU,QAAA,EAAG,ySAED,cAAAZ,IAAA,WAAAY